      "properties": {
        "person": {
          "description": "Insured person",
          "enum": [
            "primary",
            "spouse"
          ],
          "title": "Person",
          "type": "string"
        },
        "daily_benefit": {
          "description": "Daily benefit amount",
          "minimum": 0.0,
