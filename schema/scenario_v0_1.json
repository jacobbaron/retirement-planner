    "HealthcareExpenses": {
      "description": "Healthcare expenses.",
      "properties": {
        "insurance": {
          "default": 0,
          "description": "Monthly health insurance premium",
          "minimum": 0.0,
          "title": "Insurance",
          "type": "number"
        },
        "out_of_pocket": {
          "default": 0,
          "description": "Annual out-of-pocket healthcare costs",
          "minimum": 0.0,
          "title": "Out Of Pocket",
          "type": "number"
        },
        "medicare": {
          "default": 0,
          "description": "Monthly Medicare premium (when applicable)",
          "minimum": 0.0,
          "title": "Medicare",
          "type": "number"
        }
      },
      "title": "HealthcareExpenses",
      "type": "object"
    },
    "Household": {
      "description": "Household composition and demographics.",
      "properties": {
        "primary_age": {
          "description": "Age of primary household member",
          "maximum": 100,
          "minimum": 18,
          "title": "Primary Age",
          "type": "integer"
        },
        "spouse_age": {
          "anyOf": [
            {
              "maximum": 100,
              "minimum": 18,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Age of spouse (null if single)",
