          "title": "College Type",
          "type": "string"
        }
      },
      "required": [
        "birth_year",
        "college_start_year"
      ],
      "title": "Child",
      "type": "object"
    },
    "College529Account": {
      "description": "529 college savings account.",
      "properties": {
        "name": {
          "description": "Account name",
          "minLength": 1,
          "title": "Name",
          "type": "string"
        },
        "current_balance": {
          "description": "Current account balance",
          "minimum": 0.0,
          "title": "Current Balance",
          "type": "number"
        },
        "asset_allocation": {
          "$ref": "#/$defs/AssetAllocation",
          "description": "Asset allocation"
        },
        "beneficiary": {
          "description": "Name of child beneficiary",
          "title": "Beneficiary",
          "type": "string"
        }
      },
      "required": [
        "name",
        "current_balance",
        "asset_allocation",
        "beneficiary"
      ],
      "title": "College529Account",
      "type": "object"
    },
    "Correlations": {
      "description": "Correlation matrix between asset classes.",
      "properties": {
        "stocks_bonds": {
          "default": 0.2,
          "description": "Stocks-Bonds correlation",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Stocks Bonds",
          "type": "number"
        },
        "stocks_cash": {
          "default": 0.0,
          "description": "Stocks-Cash correlation",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Stocks Cash",
          "type": "number"
        },
        "bonds_cash": {
          "default": 0.1,
          "description": "Bonds-Cash correlation",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Bonds Cash",
          "type": "number"
        }
      },
      "title": "Correlations",
      "type": "object"
    },
    "CreditCard": {
      "description": "Credit card debt information.",
      "properties": {
        "name": {
          "description": "Credit card name",
          "minLength": 1,
          "title": "Name",
          "type": "string"
        },
        "current_balance": {
          "description": "Current balance",
          "minimum": 0.0,
          "title": "Current Balance",
          "type": "number"
        },
        "interest_rate": {
          "description": "Interest rate (0-1)",
          "maximum": 1.0,
          "minimum": 0.0,
          "title": "Interest Rate",
          "type": "number"
        },
        "minimum_payment": {
          "description": "Minimum monthly payment",
          "minimum": 0.0,
          "title": "Minimum Payment",
          "type": "number"
        }
      },
      "required": [
        "name",
        "current_balance",
        "interest_rate",
        "minimum_payment"
      ],
      "title": "CreditCard",
      "type": "object"
    },
    "DisabilityInsurance": {
      "description": "Disability insurance policy information.",
      "properties": {
        "person": {
          "description": "Insured person",
          "enum": [
            "primary",
            "spouse"
          ],
          "title": "Person",
          "type": "string"
        },
        "monthly_benefit": {
          "description": "Monthly benefit amount",
          "minimum": 0.0,
          "title": "Monthly Benefit",
          "type": "number"
        },
        "annual_premium": {
          "description": "Annual premium amount",
          "minimum": 0.0,
          "title": "Annual Premium",
          "type": "number"
