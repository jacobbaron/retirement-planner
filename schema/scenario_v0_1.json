      "required": [
        "person",
        "monthly_benefit",
        "annual_premium"
      ],
      "title": "DisabilityInsurance",
      "type": "object"
    },
    "ExpectedReturns": {
      "description": "Expected annual returns by asset class.",
      "properties": {
        "stocks": {
          "description": "Expected annual stock return",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Stocks",
          "type": "number"
        },
        "bonds": {
          "description": "Expected annual bond return",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Bonds",
          "type": "number"
        },
        "cash": {
          "description": "Expected annual cash return",
          "maximum": 1.0,
          "minimum": -1.0,
          "title": "Cash",
          "type": "number"
        }
      },
      "required": [
        "stocks",
        "bonds",
        "cash"
      ],
      "title": "ExpectedReturns",
      "type": "object"
