          "type": "array"
        },
        "traditional_ira": {
          "description": "Traditional IRA accounts",
          "items": {
            "$ref": "#/$defs/TraditionalIRAAccount"
          },
          "title": "Traditional Ira",
          "type": "array"
        },
        "roth_ira": {
