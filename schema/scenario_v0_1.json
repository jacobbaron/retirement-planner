          "items": {
            "$ref": "#/$defs/StudentLoan"
          },
          "title": "Student Loans",
          "type": "array"
        },
        "credit_cards": {
          "description": "Credit card debt",
          "items": {
            "$ref": "#/$defs/CreditCard"
          },
          "title": "Credit Cards",
          "type": "array"
        },
        "auto_loans": {
          "description": "Auto loan debt",
          "items": {
            "$ref": "#/$defs/AutoLoan"
          },
          "title": "Auto Loans",
          "type": "array"
        }
      },
      "title": "Liabilities",
      "type": "object"
    },
    "LifeInsurance": {
      "description": "Life insurance policy information.",
      "properties": {
        "person": {
          "description": "Insured person",
          "enum": [
            "primary",
            "spouse"
          ],
          "title": "Person",
          "type": "string"
        },
        "death_benefit": {
          "description": "Death benefit amount",
