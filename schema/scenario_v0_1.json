        "other": {
          "default": 0,
          "description": "Monthly other expenses",
          "minimum": 0.0,
          "title": "Other",
          "type": "number"
        },
        "lumpy_expenses": {
          "description": "One-time or irregular expenses",
          "items": {
            "$ref": "#/$defs/LumpyExpense"
          },
          "title": "Lumpy Expenses",
          "type": "array"
        }
      },
      "title": "Expenses",
      "type": "object"
    },
