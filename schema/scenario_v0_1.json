          "description": "Stock allocation (0-1)",
          "maximum": 1.0,
          "minimum": 0.0,
          "title": "Stocks",
          "type": "number"
        },
        "bonds": {
          "description": "Bond allocation (0-1)",
          "maximum": 1.0,
          "minimum": 0.0,
          "title": "Bonds",
          "type": "number"
        },
        "cash": {
          "description": "Cash allocation (0-1)",
          "maximum": 1.0,
          "minimum": 0.0,
          "title": "Cash",
          "type": "number"
        }
      },
      "required": [
        "stocks",
        "bonds",
        "cash"
      ],
      "title": "AssetAllocation",
      "type": "object"
    },
    "AutoLoan": {
      "description": "Auto loan information.",
      "properties": {
        "name": {
          "description": "Loan name",
          "minLength": 1,
          "title": "Name",
          "type": "string"
        },
        "current_balance": {
          "description": "Current loan balance",
          "minimum": 0.0,
          "title": "Current Balance",
          "type": "number"
        },
        "interest_rate": {
          "description": "Interest rate (0-1)",
          "maximum": 1.0,
          "minimum": 0.0,
          "title": "Interest Rate",
          "type": "number"
        },
        "term_months": {
          "description": "Loan term in months",
          "maximum": 120,
          "minimum": 1,
          "title": "Term Months",
          "type": "integer"
