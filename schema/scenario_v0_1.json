        "housing": {
          "$ref": "#/$defs/HousingExpenses",
          "description": "Housing-related expenses"
        },
        "transportation": {
          "$ref": "#/$defs/TransportationExpenses",
          "description": "Transportation expenses"
        },
        "healthcare": {
          "$ref": "#/$defs/HealthcareExpenses",
          "description": "Healthcare expenses"
        },
        "food": {
          "default": 0,
          "description": "Monthly food expenses",
          "minimum": 0.0,
          "title": "Food",
          "type": "number"
        },
        "entertainment": {
          "default": 0,
          "description": "Monthly entertainment expenses",
          "minimum": 0.0,
          "title": "Entertainment",
          "type": "number"
        },
        "travel": {
          "default": 0,
          "description": "Annual travel expenses",
          "minimum": 0.0,
          "title": "Travel",
          "type": "number"
        },
        "education": {
          "default": 0,
          "description": "Annual education expenses (for children)",
          "minimum": 0.0,
          "title": "Education",
          "type": "number"
