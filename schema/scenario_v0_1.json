    "CashAccounts": {
      "description": "Cash and cash equivalent accounts.",
      "properties": {
        "checking": {
          "default": 0,
          "description": "Checking account balance",
          "minimum": 0.0,
          "title": "Checking",
          "type": "number"
        },
        "savings": {
          "default": 0,
          "description": "Savings account balance",
          "minimum": 0.0,
