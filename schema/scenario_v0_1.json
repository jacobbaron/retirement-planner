          "type": "string"
        },
        "state": {
          "description": "State of residence (2-letter code)",
          "maxLength": 2,
          "minLength": 2,
          "title": "State",
          "type": "string"
        },
        "children": {
          "description": "List of children in household",
          "items": {
            "$ref": "#/$defs/Child"
          },
          "title": "Children",
          "type": "array"
        }
      },
      "required": [
        "primary_age",
        "filing_status",
        "state"
      ],
      "title": "Household",
      "type": "object"
    },
